{{#each this}}
<li class="photo-card">
    <img src="{{webformatURL}}" alt="{{largeImageURL}}" />

    <div class="stats">
        <p class="stats-item">
            <i class="material-icons">❤️</i>
            {{likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons">👁️‍🗨️</i>
            {{views}}
        </p>
        <p class="stats-item">
            <i class="material-icons">💬</i>
            {{comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons">📥</i>
            {{downloads}}
        </p>
    </div>
</li>
{{/each}}